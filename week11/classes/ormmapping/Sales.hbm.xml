<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Sales" table="Sales" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="totalSales" column="TotalSales" type="integer" length="11" not-null="true" lazy="false"/>
		<property name="financialYear" column="FinancialYear" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
